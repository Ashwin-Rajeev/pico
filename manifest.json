{
    "name": "Pico-Save highlights",
    "manifest_version": 2,
    "version": "0.1",
    "permissions": [
        "tabs",
        "storage"
      ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons":{"128":"icon.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}