{
    "manifest_version": 2,
    "name": "Pico-Save highlights",
    "version": "0.1",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["jquery-3.4.1.min.js","content.js"]
      }
    ],
    "browser_action": {
      "default_icon": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      }
    }
  }